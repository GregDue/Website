  <script>
    function addTwoNumbers() {
  // Prompt the user for two numbers
  const num1 = parseFloat(prompt("Enter the first number:"));
  const num2 = parseFloat(prompt("Enter the second number:"));

  // Check if the input is valid
  if (!isNaN(num1) && !isNaN(num2)) {
    const result = num1 + num2;
    alert(`The sum of ${num1} and ${num2} is: ${result}`);
  } else {
    alert("Invalid input. Please enter valid numbers.");
  }
}
  function convertFahrenheitToCelsius() {
  // Prompt the user for a temperature in Fahrenheit
  const fahrenheit = parseFloat(prompt("Enter the temperature in Fahrenheit:"));

  // Check if the input is valid
  if (!isNaN(fahrenheit)) {
    const celsius = (fahrenheit - 32) * (5 / 9);
    alert(`Temperature in Celsius: ${celsius.toFixed(2)}`);
  } else {
    alert("Invalid input. Please enter a valid temperature.");
  }
}

 function convertMetersToFeet() {
  // Prompt the user for a length in meters
  const meters = parseFloat(prompt("Enter the length in meters:"));

  // Check if the input is valid
  if (!isNaN(meters)) {
    const feet = meters * 3.281;
    alert(`Length in Feet: ${feet.toFixed(2)}`);
  } else {
    alert("Invalid input. Please enter a valid length.");
  }
}

function convertFeetToMeters() {
  // Prompt the user for a length in feet
  const feet = parseFloat(prompt("Enter the length in feet:"));

  // Check if the input is valid
  if (!isNaN(feet)) {
    const meters = feet / 3.281;
    alert(`Length in Meters: ${meters.toFixed(2)}`);
  } else {
    alert("Invalid input. Please enter a valid length.");
  }
}
  </script>
